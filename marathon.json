{
  "id": "/oinker",
  "instances": 3,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "eslng/oinker-go:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ]
    }
  },
  "cmd": "oinker --cassandra-addr=cassandra-dcos-node.cassandra.dcos.mesos --cassandra-repl=3",
  "healthChecks": [{
    "protocol": "TCP",
    "gracePeriodSeconds": 600,
    "intervalSeconds": 30,
    "portIndex": 0,
    "timeoutSeconds": 10,
    "maxConsecutiveFailures": 2
  }],
  "ports": [0],
  "cpus": 0.25,
  "mem": 256.0
}
